<div class="market-opens"><span class="market-status-text">Brasil: Mercado Fechado</span><span class="opening-time market-close"></span></div>

<script>
    define("MarketInfo", ["jquery"], function ($) {
        var isMarketOpen = 'False';
var timeToMarketChange = Number(29932983,829);
var hoursAndMinutes = '{0} h {1} min';
var minutes = '{0} min';
var seconds = '{0} seg';
var marketOpensIn = 'Brasil: Mercado Abre Em';
var marketClosesIn = 'Brasil: O Mercado Fecha Em';
        var openingTimeContainer = $(".opening-time");
        var marketStatusContainer = $(".market-status-text");

        if (isMarketOpen.length < 1 || isNaN(timeToMarketChange))
        {
            return;
        }

        var refreshInterval = 1000;
        var switchMarketState = false;
        var start = timeToMarketChange < 0 ? 0 : timeToMarketChange - (timeToMarketChange % 1000);
        var x = setInterval(function () {
            start = start - refreshInterval;
            if (start >= 7200000) {
                switchMarketState = true;
                return;
            }

            if (switchMarketState) {
                if (isMarketOpen.toLowerCase() === "true") {
                    marketStatusContainer.text(marketClosesIn);
                } else  {
                    marketStatusContainer.text(marketOpensIn);
                }

                openingTimeContainer.show();
                switchMarketState = false;
            }

            if (start <= 0) {
                openingTimeContainer.text("");
                if (isMarketOpen.toLowerCase() === "true") {
                    marketStatusContainer.text('Brasil: Mercado Fechado');
                } else {
                    marketStatusContainer.text('Brasil: Mercado Aberto');
                }
                openingTimeContainer.hide();
                clearInterval(x);
                return;
            }

            try {
                var text = "";
                if (start >= 3600000) {
                    var timeInHours = (start - (start % 3600000)) / 3600000;
                    var totalMins = start % 3600000;
                    var timeInMinutes = (totalMins - (totalMins % 60000)) / 60000;
                    text = hoursAndMinutes.replace("{0}", timeInHours);
                    text = text.replace("{1}", timeInMinutes);
                } else if (start >= 60000) {
                    var timeInOnlyMinutes = (start - (start % 60000)) / 60000;
                    text = minutes.replace("{0}", timeInOnlyMinutes);
                } else if (start >= 1000) {
                    text = seconds.replace("{0}", Math.round(start / 1000));
                }
                openingTimeContainer.show().text(text);
            } catch (e) {
                marketStatusContainer.hide();
                openingTimeContainer.hide();
            }

        }, refreshInterval);
    });
</script>
</div>
 </div>
 

