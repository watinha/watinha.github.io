<script type="text/javascript">
    function counter_click(cname,obj){
        var  http = new XMLHttpRequest(),
            url = "/api/counter/",
            params = "cname=" + cname;
        http.open("POST", url, true);

        http.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
        http.onreadystatechange = function() {
            if(http.readyState == 4 && http.status == 200) {
                var counter_message = obj.parentNode.parentNode.parentNode.nextSibling;

                if(JSON.parse(http.responseText).count == -1){
                    counter_message.innerHTML = '参与人数太多，请等会再试';
                    counter_message.style.display = '';
                    setTimeout(function() {counter_message.style.display='none'}, 2e3);
                }
                else{
                    obj.previousSibling.children[0].innerHTML = JSON.parse(http.responseText).count;
                }
            }
        }
        http.send(params);
    };
</script>


