	<!-- channel -->
	<section class="cnl" data-plate-expose="000184_tysy_yc">
        <!-- feed 开始 -->
    <div id="news_feed" data-spm='fd-d'>
        <h3 class='h ht2'> <em>为您推荐</em></h3>
        <script>
            var feed_namespace_metaDom = document.createElement('meta')
            feed_namespace_metaDom.name = "data-spm"
            feed_namespace_metaDom.content = "smwp"
            document.getElementsByTagName("head")[0].appendChild(feed_namespace_metaDom)
            document.body.setAttribute('data-spm', 'sports-index');

            var feedContainer = document.getElementById('news_feed');
            var feed_namespace_btm_lock = false;
            var feed_namespace_page = 1;
            var feed_namespace_ecode;
            if (window.sohuSpm && typeof window.sohuSpm.getCodes === 'function') {
                feed_namespace_ecode = sohuSpm.getCodes().e;
            }
            var feed_namespace_pvID = sessionStorage.getItem('session_pv_id') || feed_namespace_ecode || 0;
            sessionStorage.setItem('session_pv_id', feed_namespace_pvID);
            var feed_namespace_feedCache = sessionStorage.getItem('feed-' + feed_namespace_pvID);
            var feed_namespace_scrollCache = sessionStorage.getItem('sports_index_scrollTop');
            if (feed_namespace_feedCache) {
                console.log(1);
                feed_namespace_genDiv(JSON.parse(feed_namespace_feedCache));
                setTimeout(() => {
                    window.scrollTo(0, feed_namespace_scrollCache);
                })
            }
            function feed_namespace_genDiv(res) {
                var con_frag = document.createDocumentFragment();
                res.forEach(function (d) {
                    if (!d.cover) return;
                    var li_el = document.createElement('li');
                    li_el.style = 'position: relative;padding: 12px 15px;border-bottom: 1px solid #e8e8e8;';
                    li_el.innerHTML = "<a style=\"display: flex;font-family: PingFang SC,Lantinghei SC,Microsoft Yahei,Hiragino Sans GB,Microsoft Sans Serif,WenQuanYi Micro Hei,sans-serif;-webkit-font-smoothing: antialiased;\" href="http://m.sohu.com/c/3/\&quot;&quot;" + d.url + "\"><div style=\"position: relative; -webkit-box-flex: 1; -ms-flex: 1; flex: 1;\"><h3 style=\" margin-top:0px;padding:0 10px 0 0;margin-right: 10px; font-size: 18px; line-height: 1.3em; -webkit-line-clamp: 2; overflow: hidden; text-overflow: ellipsis; display: -webkit-box; display: box; -webkit-box-orient: vertical; color: #212121;\">" + d.mobileTitle + "</h3><div style=\" overflow: hidden; color: #999; font-size: 12px; position: absolute; bottom: 0; width: 100%; -webkit-box-sizing: border-box; box-sizing: border-box; padding-right: 20px;\"><div style=\"float: left; max-width: 10em; line-height: 1.05em; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;\">" + d.authorName + "</div><div style=\" float: left; position: relative; line-height: 1.05em; padding-left: 0.4rem;\"> · " + feed_namespace_getDateDiff(d.publicTime) + "</div></div></div><div style=\"background-image: url(" + d.cover + ");  position: relative; -ms-flex-negative: 0; flex-shrink: 0; width: 32.75346%; padding-top: 21.55%; background-size: cover;\"></div></a>";
                    con_frag.appendChild(li_el);
                });
                feedContainer.appendChild(con_frag);
                feed_namespace_btm_lock = false;
                feed_namespace_page += 1;
            }
            function feed_namespace_getDateDiff(dateTimeStamp) {
                var minute = 60 * 1000;
                var hour = minute * 60;
                var day = hour * 24;
                var month = day * 30;
                var year = day * 365;
                var now = parseInt(new Date().getTime());
                var diffValue = now - dateTimeStamp;
                var yearC = diffValue / year;
                var monthC = diffValue / month;
                var weekC = diffValue / (7 * day);
                var dayC = diffValue / day;
                var hourC = diffValue / hour;
                var minC = diffValue / minute;
                var result = 0;
                if (yearC >= 1) {
                    result = '' + parseInt(monthC) + '年前';
                } else if (monthC >= 1) {
                    result = '' + parseInt(monthC) + '个月前';
                } else if (weekC >= 1) {
                    result = '' + parseInt(weekC) + '周前';
                } else if (dayC >= 1) {
                    result = '' + parseInt(dayC) + '天前';
                } else if (hourC >= 1) {
                    result = '' + parseInt(hourC) + '个小时前';
                } else if (minC >= 1) {
                    result = '' + parseInt(minC) + '分钟前';
                } else {
                    result = '刚刚';
                }
                return result;
            }
            window.addEventListener('scroll', function (e) {
                sessionStorage.setItem('sports_index_scrollTop', $(window).scrollTop());
                if ($(window).scrollTop() + $(window).height() >= $(document).height() && !feed_namespace_btm_lock) {
                    feed_namespace_btm_lock = true;
                    $.ajax({
                        url: '//v2.sohu.com/integration-api/mix/region/5?page=' + feed_namespace_page + '&size=20&pvId=' + feed_namespace_pvID,
                        type: 'GET',
                        crossDomain: true,
                        xhrFields: { withCredentials: true },
                        success: function (res) {
                            feed_namespace_genDiv(res.data);
                            let _a = sessionStorage.getItem('feed-' + feed_namespace_pvID) || '[{}]';
                            _a = _a.slice(0, -1) + ',' + JSON.stringify(res.data).slice(1);
                            sessionStorage.setItem('feed-' + feed_namespace_pvID, _a);
                            if (window.sohuSpm && typeof window.sohuSpm.domDidChange === 'function') {
                                window.sohuSpm.domDidChange({ target: 'fd-d' });
                            }
                        }
                    });
                }
            })
        </script>
        
    </div>
    <!-- feed 结束 -->
    <script src="http://statics.itc.cn/spm/prod/js/1.0.1/index.js"></script>
    



