P.when('A', 'GLUXWidget').execute(function(A, GLUXWidget) {
  A.on('packard:glow:destinationChangeAll', function(){
    var newPath = window.location.pathname;
    var refRegEx = new RegExp("/ref=.*?(/|$)", "i");
    if (!refRegEx.test(newPath)) {
        newPath = addRefToPath(newPath);
    }
    var newPath = newPath.replace(refRegEx, "/ref=glow_cls" + '$1');
    var refreshURL = window.location.href.replace(window.location.pathname, newPath);
    refreshURL = searchAndRemove("page", refreshURL);
    refreshURL = searchAndRemove("spIA", refreshURL);
    window.location.href = refreshURL;
    return false;
  });

  var searchAndRemove = function(searchTerm, source) {
     var searchRegEx = new RegExp("([?&])" + searchTerm + "=.*?(&|$)", "i");
     if (source.match(searchRegEx)) {
         source = source.replace(searchRegEx, '$1');
     }
     return source;
  }

  var addRefToPath = function(path) {
    if (path.slice(-1) !== '/') {
        path += '/';
    }
        path += 'ref=';
        return path;
  }

});
P.when( 'A').execute( 'playSampleFunctions', function(A) {

  var $ = A.$,
      $sampleButtons = $( '.mp3-sample-buttons' );
  
  $sampleButtons.delegate( '.mp3-sample-action', 'click', sampleButtonClickHandler );
  $sampleButtons.delegate( '.mp3-sample-action-text', 'click', sampleTextClickHandler );
  $sampleButtons.find( 'audio').bind( 'ended', sampleFinishedPlayingHandler );

   
  function sampleButtonClickHandler( e ) 
  {
    var $el = $( this ),
        $audioElement = $el.siblings( 'audio' ).eq( 0 ); 
    if ( $audioElement.length === 0 )
    {
      return;
    }
    else
    {
      
      e.preventDefault();
      
      playOrStopSample( $el, $audioElement[0] );
    }
  }
  
  
  function sampleTextClickHandler( e )
  {
    e.preventDefault();
    $( this ).siblings( '.mp3-sample-action' ).eq( 0 ).trigger( 'click' );
  }
  
  
  function playOrStopSample( $el, audioElement )
  { 
    var action = $el.attr( 'data-action' ),
        $textEl = $el.siblings( '.mp3-sample-action-text' ).eq( 0 ),
        isPlay = action === 'play',
        isStop = action === 'stop';
    
    
    if (!isPlay && !isStop)
    {
      return;
    }

    if (isStop)
    {
      audioElement.pause();
    }
    else if (isPlay) 
    {
      stopCurrentlyPlayingSamples();

      audioElement.play();
    }
    
    updateAudioUI( $el, $textEl, isPlay );
  }

  
  function updateAudioUI( $audioButtonEl, $textEl, isPlayAction )
  {
    var classToHide = isPlayAction ? '.triangle' : '.rect',
        classToShow = isPlayAction ? '.rect' : '.triangle',
        textAttribute = isPlayAction ? 'pausetext' : 'playtext',
        newAction = isPlayAction ? 'stop' : 'play';

    $audioButtonEl.attr( 'data-action', newAction );
    
    
    $audioButtonEl.find( classToHide ).addClass( 'sx-hidden' );
    $audioButtonEl.find( classToShow ).removeClass( 'sx-hidden' );
    
    $textEl.text( $textEl.attr( 'data-' + textAttribute ) );
  }

  
  function stopCurrentlyPlayingSamples()
  {
    var $currentlyPlayingSample = $('.mp3-sample-buttons').find('span[data-action="stop"]'),
        $currentlyPlayingAudioElement = $currentlyPlayingSample.siblings('audio');

    if ($currentlyPlayingAudioElement.length == 1) 
    {
      $currentlyPlayingAudioElement[0].pause();

      var $textEl = $currentlyPlayingSample.siblings( '.mp3-sample-action-text' ).eq( 0 );

      updateAudioUI( $currentlyPlayingSample, $textEl, false);
    }
  }

  
  function sampleFinishedPlayingHandler( e )
  {
    var $el = $( e.currentTarget ).closest( '.mp3-sample-buttons' ).find( '.mp3-sample-action' ),
        $textEl = $el.siblings( '.mp3-sample-action-text' ).eq( 0 );
    
    updateAudioUI( $el, $textEl, false );
  }
});


P.when( 'A', 'dmusic_webstore_prime' ).execute( 'addToLibraryFunctions', function( A, dmusic ) {
  var $ = A.$;
  
  
  dmusic.DigitalMusicRobinBtnManager.register();
  
  
  window.addToCloudPlayerCallback = function(result, robinButton) {
    
    var $primeButton = robinButton.button.closest('.music-prime-button');
        $primeButton.css('visibility', 'hidden');
        $primeButton.parent().find('.music-play-button').removeClass('sx-hidden').css('display','inline-block');

    $primeButton.remove();
  };
});


P.when( 'A' ).execute( 'playButtonFunctions', function( A ) {
  var $ = A.$;

  $( '.music-buttons' ).delegate( '.music-play-button button', 'click', playButtonClickHandler );
  function playButtonClickHandler()
  {
    var $el = $( this );
    
    var asinObj = {
      type: $el.data( 'type' ),
      title: $el.data( 'title' ),
      asin: $el.data( 'asin' )
    };
    
    
    if (asinObj.type === 'playlist') {
      return true;
    }
    else if ($( 'html.a-ios' ).length > 0) {
        executeiOSAmazonMusicApp(asinObj);
    }
    else if ($( 'html.a-android' ).length > 0) {
        executeAndroidAmazonMusicApp(asinObj);
    }
  }
  
  var wallTimeToReturnToApp = 4000;
  var threadTimeout = 1000; 
  function executeiOSAmazonMusicApp(asinObj) {
      var appStoreUri = "itms://itunes.apple.com/us/app/amazon-cloud-player/id510855668?mt=8";
      var appUri = "amznmp3://mp3/library/cirrus/albums/album?source=store%album%%track%&sync=true&autoPlay=1";

      
      var queryParams = '';
      if (asinObj.type === 'album') {
          queryParams = '&albumASIN=' + asinObj.asin + '&albumName=' + encodeURIComponent(asinObj.title);
          appUri = appUri.replace('%track%', '').replace('%album%', queryParams);
      } else if (asinObj.type === 'track') {
          queryParams = '&trackASIN=' + asinObj.asin + '&trackName=' + encodeURIComponent(asinObj.title);
          appUri = appUri.replace('%track%', queryParams).replace('%album%', '');
      }
      
      
      var start = new Date().getTime();
      setTimeout(function() {
          if ((new Date().getTime() - start) < wallTimeToReturnToApp) {
              window.location = appStoreUri;
          }
      }, threadTimeout);

      window.location = appUri;
  }

  
  function executeAndroidAmazonMusicApp(asinObj) {
      threadTimeout = 2500;
      var amznMp3Link = "amznmp3://library/album/asin/%albumAsin%?action=playNowPlaying%trackAsin%&sync=true";
      var marketUri = "market://details?id=com.amazon.mp3";
      var id = '#cloud-player-app-opener';
      var el;
      
      if (asinObj.type === 'album') {
          amznMp3Link = amznMp3Link
                          .replace('%albumAsin%', asinObj.asin)
                          .replace('%trackAsin%', '');
      } else if (asinObj.type === 'track') {
          amznMp3Link = amznMp3Link
                          .replace('%albumAsin%', '')
                          .replace('%trackAsin%', '&trackAsin=' + asinObj.asin);
      }
      
      
      var chromeVersion = (function() {
          var versionReg = window.navigator.appVersion.match(/Chrome\/(\d+)\./);
          var versionMatches;

          if (versionReg && versionReg.length > 1) {
              versionMatches = versionReg[1];
          }

          if (versionMatches) {
              return parseInt(versionMatches, 10);
          }

          return null;
      }());

      
      if (chromeVersion && chromeVersion >= 25) {
          
          if (amznMp3Link.indexOf("amznmp3://") === 0) {
              amznMp3Link = "intent://" + amznMp3Link.split("amznmp3://")[1] + "#Intent;scheme=amznmp3;package=com.amazon.mp3;end";
          }

          var anchor = document.createElement('a');
          anchor.id = id;
          anchor.href = amznMp3Link;
          document.body.appendChild(anchor);
          anchor.click(); 
          el = anchor;
      } else {
          var iframe = document.createElement('iframe');
          iframe.frameBorder = 0;
          iframe.width = '0px';
          iframe.height = '0px';
          iframe.id = id;
          document.body.appendChild(iframe);
          iframe.setAttribute('src', amznMp3Link);
          el = iframe;
      }

      var start = new Date().getTime();
      setTimeout(function() {
          $(el).remove();
          if ((new Date().getTime() - start) < wallTimeToReturnToApp) {
              window.location = marketUri;
          }
      }, threadTimeout);
  }
});




P.when('fresh-add-to-cart').execute(function(freshATC) {
    

    function updateNumberInCartBadge(event) {
        var asin = event.data.asin;
        var addToCartContainer = event.$target.parents().find(".s-slick-item-container").find("[data-asin='" + asin + "']").find(".s-slick-add-to-cart-badge-position");

        if(!addToCartContainer) {
            return;
        }
        addToCartContainer.removeClass("aok-hidden");

        var quantitySpan = addToCartContainer.find(".s-slick-added-to-cart-badge");
        if(!quantitySpan) {
            return;
        }
        var quantity = parseInt(quantitySpan.text(), 10);
        quantitySpan.text(quantitySpan.text().replace(quantity, quantity + 1));
    }

    function updateCoupon(event, response) {
        var couponClipStatus = response == undefined ? undefined : response.couponClipStatus;
        if (couponClipStatus == "CLIP_SUCCESSFUL" || couponClipStatus == "ALREADY_CLIPPED") {
            var $vpcContainer = event.$target.closest(".s-slick-item-container").find(".fresh-vpc-container");
            if ($vpcContainer !== undefined) {
                var $unclippedVpc = $vpcContainer.find(".fresh-vpc-unclipped");
                var $clippedVpc = $vpcContainer.find(".fresh-vpc-clipped");
                if ($unclippedVpc !== undefined && $clippedVpc !== undefined && $unclippedVpc.is(":visible")) {
                    $unclippedVpc.hide();
                    $clippedVpc.show();
                }
            }
        }
    }

    freshATC.addToFreshCart({
        success: function(event, response) {
            if((response !== null) && response.isOK) {
                updateNumberInCartBadge(event);
                updateCoupon(event, response);
                
            }
        },
        error: function(event, jqXHR, textStatus, errorThrown) { P.log("Error adding to cart on fresh search page"); }
    });
});


P.when('A').execute(function(A) {
    A.declarative('s-deep-link', 'click', function(event) {
    /**
     * There's no good way to check if the aiv app is installed. We can
     * try to open the app. If it isn't there we can then redirect them
     * to app store to download the app.
     * Logic copy from /RetailSearchPageletGroup/WebContent/WEB-INF/tags/mobile/smart/slick/script/DigitalMusicModule.tag
     **/
        event.$event.stopPropagation();
        event.$event.preventDefault();
        var start = new Date().getTime();
        setTimeout(function() {
            if ((new Date().getTime() - start) < 4000) {
                window.location = event.data.aivAppStoreUrl;
            }
        }, 1000);
        window.location = event.data.aivAppUrl;
    });
});
</script>

