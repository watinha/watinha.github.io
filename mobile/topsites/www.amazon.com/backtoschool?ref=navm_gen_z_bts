<script>
    try {
        P.register('dpImageCachingUtils', function(){
            return {
                isEmptyString : function(str) {
                    return (typeof str === 'undefined' || str === null || str === '');
                },
        
                getRegexOutput : function(str, regex) {
                    var matchedString;
                    if (!this.isEmptyString(str)) {
                        var matchedList = str.match(regex);
                        if ((matchedList) && (matchedList.length >= 1)) {
                            matchedString = matchedList[0];
                        }
                    }
                    return matchedString;
                }
            };
        });
    } catch(err) {
    }
</script>




<script>
    P.when('jQuery', 'dpImageCachingUtils', 'load').execute(function($, dpImageCachingUtils){

        var DP_IMG_CACHE_HINT = "&ref=plSrch";
        var HASH_MAP_CSS_CLASS = "imgUrlHash";
        var ASIN_RESULT_CSS_CLASS = "aw-search-results";
        var DP_IMG_URL_CACHING_FAILED = "sxDpImagesCachingFailed";
        var DP_IMG_URL_CACHED = "sxDpImagesCached";
        var SX_APPENDING_HINT_IN_DP_URL_FAILED = "sxAppendingHintInDpUrlFailed";
        
        var appendHintInDpURL = function(dpLink, pId) {
            var dpURL = dpLink.attr("href");
            if (!dpImageCachingUtils.isEmptyString(dpURL)) {
                var dpUrlHashParam;
                if (dpURL.indexOf("#") != -1) {
                    var dpURLParams = dpURL.split("#");
                    if(dpURLParams.length >= 1) {
                        dpURL = dpURLParams[0];
                        if(dpURLParams.length >= 2) {
                            dpUrlHashParam = dpURLParams[1];
                        }
                    }
                }
                
                dpURL = dpURL + ((dpURL.indexOf("?") == -1) ? "?" : "&");
                dpURL = dpURL + "dpPl=1" + "&dpID=" + pId + DP_IMG_CACHE_HINT;
                
                if (!dpImageCachingUtils.isEmptyString(dpUrlHashParam)) {
                    dpURL = dpURL + "#" + dpUrlHashParam;
                }
                
                dpLink.attr("href", dpURL);
                if(ue && ue.count) {
                    ue.count(DP_IMG_URL_CACHED, (ue.count(DP_IMG_URL_CACHED) || 0) + 1);
                }
            }
        }
        
        var getPhysicalId = function(imageUrl) {
            if (!dpImageCachingUtils.isEmptyString(imageUrl)) {
                var physicalIdRegex = /[^\/]+$/;
                var physicalIdWithStyleCodes = dpImageCachingUtils.getRegexOutput(imageUrl, physicalIdRegex);
                var physicalId;
                if(!dpImageCachingUtils.isEmptyString(physicalIdWithStyleCodes)) {
                    var physicalIdSplitList = physicalIdWithStyleCodes.split(".");
                    if(physicalIdSplitList.length >= 1) {
                        physicalId = physicalIdSplitList[0];
                    }
                }
                return physicalId;
            }
        }
        
        var populateImgUrlHashMap = function() {
            var asinToImgUrlHashMap = {};
            $("."+HASH_MAP_CSS_CLASS).each(function() {
                var hashMapRow = $(this);
                var asinData = hashMapRow.attr("data-asin");
                var imgUrlData = hashMapRow.attr("data-url");
                if((!dpImageCachingUtils.isEmptyString(asinData)) && (!dpImageCachingUtils.isEmptyString(imgUrlData))) {
                    asinToImgUrlHashMap[asinData] = imgUrlData;
                }
            });
            return asinToImgUrlHashMap; 
        }
        
        var setDpImgUrlsForCaching = function() {
            var asinToImgUrlHashMap = populateImgUrlHashMap();
            
            $("."+ASIN_RESULT_CSS_CLASS).each(function(){ 
                var itemLinkElement = $(this);
                var asinData = itemLinkElement.attr("data-asin");
                if(asinToImgUrlHashMap.hasOwnProperty(asinData)) {
                    var dpImgURL = asinToImgUrlHashMap[asinData];
                    if(!dpImageCachingUtils.isEmptyString(dpImgURL)) {
                        var pId = getPhysicalId(dpImgURL);
                        try {
                            var img = new Image();
                            img.src = dpImgURL;
                            if($.isFunction(appendHintInDpURL)) {
                                $(img).load( function (){appendHintInDpURL(itemLinkElement, pId)});
                            }
                        } catch (err) {
                            if(ue && ue.count) {
                                ue.count(SX_APPENDING_HINT_IN_DP_URL_FAILED, (ue.count(SX_APPENDING_HINT_IN_DP_URL_FAILED) || 0) + 1);
                            }
                        }
                    }
                }     
            });
        }

        try {
            setDpImgUrlsForCaching();
        } catch (err) {
            if(ue && ue.count) {
                ue.count(DP_IMG_URL_CACHING_FAILED, (ue.count(DP_IMG_URL_CACHING_FAILED) || 0) + 1);
            }
        }
    });
</script>





</div>
                
                <div data-view-section-precache="center" data-content-cache-key-gen="javascript:return 'hidden';"></div>
                </div></div>
            
