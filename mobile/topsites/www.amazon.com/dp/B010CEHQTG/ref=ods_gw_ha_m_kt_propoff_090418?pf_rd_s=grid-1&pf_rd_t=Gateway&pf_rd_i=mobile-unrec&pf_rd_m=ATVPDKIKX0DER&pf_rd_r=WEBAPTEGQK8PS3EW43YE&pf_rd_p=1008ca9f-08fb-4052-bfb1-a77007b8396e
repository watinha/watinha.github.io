ue.count(metricName, 1);
ue.count(metricName + "Time", Date.now() - window.ue_t0);
}
},
logSpecificCount : function(metricName, metricValue){
if(window.ue && ue.count) {
ue.count(metricName, metricValue);
}
}
};
});
