<script type="text/javascript">
/* This code segment is taken (mostly) from ./mobile/dimension.jsp, and the code should
be removed from there while cleaning up the weblab */
P.guardError("TwisterInlineJSMobileButtonClick", function () {
var buttonIds = {},
ueCounterNameAUI = "Twister:buttonClickTillAUIMobile",
ueCounterNameTwister = "Twister:buttonClickTillTwisterMobile",
ueCounterIngressClickCounter = "dpmobiletwisteringressclickcounter",
ueIngressNotClicked = "dpmobiletwisteringressnotclicked",
blankTwisterSecViewCounter = "blankTwisterSecondaryView",
AUIMobileLoaded = 0,
TwisterMobileLoaded = 0;
logCounter(ueIngressNotClicked, 1);
buttonIds["configuration"] = "configuration-button";
buttonIds["color_name"] = "color_name-button";
for (var dimKey in buttonIds) {
var buttonObj = document.getElementById(buttonIds[dimKey]);
if (buttonObj && typeof buttonObj.addEventListener === 'function') {
buttonObj.addEventListener("click", onButtonClick);
}
}
var dimensionKeyList = {
"dimensionKeys": buttonIds,
}
P.when("A").execute(function (A) {
AUIMobileLoaded = 1;
A.state('dimensionKeyList', dimensionKeyList)
});
P.when("mobile-twister").execute(function () {
TwisterMobileLoaded = 1;
});
function onButtonClick() {
//log Button Click Till AUI
!AUIMobileLoaded && logCounter(ueCounterNameAUI);
//log Button Click Till Twister
!TwisterMobileLoaded && logCounter(ueCounterNameTwister);
//log Click on ingress
logCounter(ueCounterIngressClickCounter);
logCounter(ueIngressNotClicked, 0);
}
function logCounter(counterKey, valueToLog) {
if (ue && ue.count) {
if (valueToLog === undefined) {
ue.count(counterKey, (ue.count(counterKey) || 0) + 1);
} else {
ue.count(counterKey, valueToLog);
}
}
}
})();
</script>
</div>
</div>
