P.when('jQuery','A', 'a-popover-base', 'a-popover-ajax').execute(function($, A, popovers, popoverAjax) {
A.on("a:popover:beforeShow:mabb-product-view-B01IQEPVLE", function(data){
P.now('dpmob-sec-view-instrumentation').execute(function(secViewInstrumentation){
if(typeof secViewInstrumentation !== "undefined") {
// Record the click time before showing the secondary view.
secViewInstrumentation.recordClickTime('');
}
});
var popover = data.popover;
var metaDataMap=popover.data;
var useStateData = metaDataMap['useStateData'];
var secViewId = metaDataMap['secViewId'];
var secViewData;
if(useStateData){
var secViewDataMap = A.state(secViewId);
secViewData = secViewDataMap["data"];
}else {
secViewData = $("#"+secViewId).html();
}
popover.update(secViewData);
});
A.on("a:popover:beforeUpdatePosition:mabb-product-view-B01IQEPVLE", function(data){
var popover = data.popover;
var metaDataMap=popover.data;
var useStateData = metaDataMap['useStateData'];
var secViewId = metaDataMap['secViewId'];
var secViewData;
if(useStateData){
var secViewDataMap = A.state(secViewId);
secViewData = secViewDataMap["data"];
} else {
secViewData = $("#"+secViewId).html();
}
var content = popover.getContent();
var $secViewContainer = $(content).find('.secondary-view-container');
if(!($secViewContainer.length === 0)) {
$secViewContainer.html(secViewData);
}
});
A.on("a:popover:afterShow:mabb-product-view-B01IQEPVLE", function(data){
var featureName = '';
