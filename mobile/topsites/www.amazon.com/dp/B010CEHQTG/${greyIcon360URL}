                    // update the accessory upsell JSON with data from digital subscription JSON
                    var digitalSubscriptionJSON = {"B01IEKNAPY":{}};
                    if (digitalSubscriptionJSON) {
                        for (var asin in accessoriesJSON) {
                            for (var childAsin in accessoriesJSON[asin]) {
                                var asinData = accessoriesJSON[asin][childAsin];

                                if (asinData.isDigitalSubscription) {
                                    var digitalSubscriptionData = digitalSubscriptionJSON[childAsin][asinData.subscriptionSku];
                                    if (digitalSubscriptionData) {
                                        var digitalSubscriptionBasePrice = digitalSubscriptionData.basePrice;

                                        asinData.buyingPriceDecimal = digitalSubscriptionBasePrice.price;
                                        asinData.buyingPrice = digitalSubscriptionBasePrice.formattedPrice;
                                        asinData.offerListingID = digitalSubscriptionData.offerListingID;
                                        asinData.featureBullets = digitalSubscriptionData.bullets;
                                        asinData.fixedCustomCartAttribute = digitalSubscriptionData.fixedCustomCartAttribute;
                                        asinData.customAttributeName = digitalSubscriptionData.customCartAttributeName;
                                        asinData.customAttributeValue = digitalSubscriptionData.basePriceCustomAttributeValue;
                                        if (asinData.isForIntroOrFreePromotion && (digitalSubscriptionData.freePricePresent || digitalSubscriptionData.introPricePresent)) {
                                            asinData.customAttributeValue = digitalSubscriptionData.offerPriceCustomAttributeValue;
                                        }

                                        accessoriesJSON[asin][childAsin] = asinData;
                                    } else {
                                        delete accessoriesJSON[asin];
                                    }
                                }
                            }
                        }
                    }

                    //Initialize DABB
                    var dabbInstance = AccessoryUpsellFactory.get($("#accessory-upsell"), accessoriesJSON);
                });
