<script type="text/javascript">
  function actionCheckout() {
    var ptc = document.getElementsByName('proceedToCheckout')[0];
    if (ptc && ptc.tagName !== "A") {
    /* tag of ptc:
     * button/input  => checkout
     *   a           => addon confirm
     */
      ptc.click();
    } else {
      // for addon-confirm (below addon threshold) and continue-shopping (all addons)
      var link = ptc || document.getElementsByName('continueShopping')[0];
      var url = link.href;
      if (ptc && (url.match("addon-confirm") || url.match("pantry-interstitial"))) {
        var isToBeGiftWrapped = document.getElementsByName("isToBeGiftWrapped")[0];
        if (isToBeGiftWrapped && isToBeGiftWrapped.checked) {
          url += "?isToBeGiftWrapped=1";
        }

        var hasPrimeGift = document.getElementsByName("hasPrimeGift")[0];
        if (hasPrimeGift && hasPrimeGift.value) {
          url += "?hasPrimeGift=1";
        }
        var hasKindleUnlimitedGift = document.getElementsByName("hasKindleUnlimitedGift")[0];
        if (hasKindleUnlimitedGift && hasKindleUnlimitedGift.value) {
          if (url.indexOf("?") > -1) {
            url += "&";
          } else {
            url += "?";
          }
          url += "hasKindleUnlimitedGift=1";
        }
      }

      window.location.href = url;
    }
  }
  P.register('mini-buy-box-ready', function(){return {}});
</script>

<div id="sc-mini-buy-box" class="sc-invisible-when-no-js">





</div>
<script type="text/javascript">
var CartPerf = (function() {
  var _hasUedata = (typeof uet == 'function');
  var _startedLoading = false;

  var loading = function() {
    if (!_startedLoading && _isReportingEvents()) {
      uet('cb');
    }
    _startedLoading = true;
  };

  var aboveTheFoldLoaded = function() {
    if (_startedLoading && _isReportingEvents()) {
      uet('af');
    }
  };

  var criticalFeatureLoaded = function() {
    if (_startedLoading && _isReportingEvents()) {
      uet('cf');
    }
  };

  var x1Loaded = function () {
    if (_startedLoading && _isReportingEvents()) {
      uet('x1');
    }
  }

  var _isReportingEvents = function() {
    return _hasUedata;
  };

  var _hasUEForAJAXRequest = (typeof ues == 'function') && (typeof uex == 'function');
  var _actionNumber = 0;
  var _scope = "cart-ajax";

  var getScope = function() {
    return _scope;
  }

  var resetUEForAJAXRequest = function() {
    _actionNumber++;
    _scope = "cart-ajax-" + _actionNumber;
    if (window.ue && _hasUedata && _hasUEForAJAXRequest) {
      uet("tc", _scope);         // set time-of-click
      ues("ctb", _scope, "1");   // set ctb flag
    }
  }

  // set start time
  var setStartTimeForAJAXRequest = function() {
    if (window.ue && _hasUedata && _hasUEForAJAXRequest) {
      ues("t0", _scope, new Date());
    }
  }

  // set request ID
  var setIDForAJAXRequest = function(rid) {
    if (window.ue && _hasUedata && _hasUEForAJAXRequest) {
      ues("id", _scope, rid);
    }
  }

  // record the event of critical feature loaded
  var cfLoadedForAJAXRequest = function() {
    if (window.ue && _hasUedata && _hasUEForAJAXRequest) {
      uet("cf", _scope);
    }
  }

  // record the event of page loaded
  var pageLoadedForAJAXRequest = function(scope) {
    if (window.ue && _hasUedata && _hasUEForAJAXRequest) {
      if (scope != null) {
        uex("ld", scope);
      } else {
        uex("ld", _scope);
      }
    }
  }

  loading();

  return {
    aboveTheFoldLoaded:         aboveTheFoldLoaded,
    cfLoadedForAJAXRequest:     cfLoadedForAJAXRequest,
    criticalFeatureLoaded:      criticalFeatureLoaded,
    x1Loaded:                   x1Loaded,
    getScope:                   getScope,
    pageLoadedForAJAXRequest:   pageLoadedForAJAXRequest,
    resetUEForAJAXRequest:      resetUEForAJAXRequest,
    setIDForAJAXRequest:        setIDForAJAXRequest,
    setStartTimeForAJAXRequest: setStartTimeForAJAXRequest
  };
})();
</script>


