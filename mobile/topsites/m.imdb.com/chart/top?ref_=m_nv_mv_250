if (window.performance && PerformanceNavigation &&
window.performance.navigation.type == PerformanceNavigation.TYPE_BACK_FORWARD) {
generic.monitoring.record_event('mobile_ad_get_ad_back_forward', false);
}
}
</script>
<!-- end TOP_AD -->

	



